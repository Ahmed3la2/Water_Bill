@model Rreal_Estate_Types;
@{
    ViewBag.Title = "إضافه عقار";

}

<form asp-action="Create" class="m-1">
    <div class="col-12 col-md-6">
        <div class="form-group">
            <label>رمز العقار</label>
            <input type="text" class="form-control" asp-for="Rreal_Estate_Types_Code">
            <span asp-validation-for="Rreal_Estate_Types_Code" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">وصف العقار</label>
            <input type="text" class="form-control" asp-for="Rreal_Estate_Types_Name">
            <span class="text-danger" asp-validation-for="Rreal_Estate_Types_Name"></span>
        </div>
        <button type="submit" class="btn btn-success">حفظ</button>
        <button class="btn btn-danger" asp-action="Index">انهاء</button>
    </div>
</form>

<style>
    .container {
        max-width: 849px;
    }
</style>
